sudo mount -o loop sdimage /mnt
./mkf cat
./mkf cp
./mkf grep
./mkf init
./mkf l2u
./mkf login
./mkf ls
./mkf more
./mkf sh
./mkf man

sudo cp -av passwd /mnt/etc
sudo ls -lt /mnt/bin
sudo umount /mnt

rm $1 *.o

echo ready to go?
read dummy

qemu-system-arm -M versatilepb -m 256M -sd sdimage -kernel wanix \
-serial mon:stdio -serial /dev/pts/1
